 function magic_multiply(x, y) {
      if (x == 0 && y == 0) {
        return `All inputs ${x * y}`;
      }
      if (Array.isArray(x)) {
        for (let i = 0; i < x.length; i++) {
          x[i] *= y;
        }
        return x;
      }
      if (typeof y === 'string') {
        return "Error: Can not multiply by string";
      }
      if (typeof x === 'string') {
        string_new = '';
        while (y > 0) {
          string_new += x;
          y--;
        }
        return string_new
      }
      return x * y;
    }
